#define RED D1
#define YELLOW D2
#define BLUE D5

void setup() {
  pinMode(RED, OUTPUT);
  pinMode(YELLOW, OUTPUT);
  pinMode(BLUE, OUTPUT);
}

void loop() {
  // RED light
  digitalWrite(RED, HIGH);
  digitalWrite(YELLOW, LOW);
  digitalWrite(BLUE, LOW);
  delay(5000);

  // YELLOW light
  digitalWrite(RED, LOW);
  digitalWrite(YELLOW, HIGH);
  digitalWrite(BLUE, LOW);
  delay(2000);

  // GREEN / BLUE light
  digitalWrite(RED, LOW);
  digitalWrite(YELLOW, LOW);
  digitalWrite(BLUE, HIGH);
  delay(5000);
}
